        function GetcountPendingRetailer() {
            window.showLoadingEnabled = false;
            $.ajax({
                type: "POST",
                url: "@Url.Action("GetCount", "Home")",
                data: {},
                success: function (data) {
                    window.showLoadingEnabled = true;
                    if (data != null) {

                        $(".getCount").html(data);

                    }

                }

            });

        }

        function Getcount() {
            $.ajax({
                type: "POST",
                url: "@Url.Action("GetCount", "Home")",
                data: {},
            success: function (data) {
                if (data != null) {

                    $(".getCount").html(data);

                }

            }

        });

        }

        $(document).ready(function () {
            Getcount();
          var TeamID = @HttpContext.Current.Session["TeamID"];
            if (TeamID == 4) {
                $("#WASA").removeAttr("style");
            }
            else
            {
                $("#KSB").removeAttr("style");
            }
        });
    </script>

