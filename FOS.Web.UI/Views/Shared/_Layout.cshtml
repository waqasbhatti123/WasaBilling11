    <style>
        .loader {
            position: fixed;
            top: 50%;
            left: 50%;
            margin-top: -50px;
            margin-left: -50px;
            width: 128px;
            height: 128px;
            z-index: 9999;
            background-color: transparent !important;
            background: url('@string.Format("{0}/Content/images/ajax-loader.gif",FOS.Web.UI.Common.Settings.AppPath)') 50% 50% no-repeat rgb(249,249,249);
        }
        #TerritorialsJobs
        {
            cursor: pointer;
        }
        .Marqueetag {
            padding-top: 18px;
            color: white;
            font-size: 17px;
            padding-right: 0px;
            padding-left: 0px;
            width: 880px;
        }
        .Marqueetag2 {
